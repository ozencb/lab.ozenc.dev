{
  "name": "@lab.ozenc.dev/shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for lab.ozenc.dev monorepo",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "typescript",
    "types",
    "monorepo",
    "shared"
  ],
  "author": "lab.ozenc.dev",
  "license": "MIT"
} 
